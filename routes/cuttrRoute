const express = require('express')
const { getCuttr, postCuttr, putCuttr, deleteCuttr } = require('../controllers/cuttrController')

const cuttrRoute = express.Router()

cuttrRoute.get('/', (req, res) => {
    getCuttr(req, res)
})

cuttrRoute.post('/', (req, res) => {
   postCuttr(req, res)
})

cuttrRoute.put('/', (req, res) => {
    putCuttr(req, res)    
})

cuttrRoute.delete('/', (req, res) => {
    deleteCuttr(req, res)
})



module.exports = cuttrRoute